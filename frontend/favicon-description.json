

{
	"masterPicture": "public/aserto-logo.svg",
	"iconsPath": "/favicons",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#a8c9e8",
			"margin": "35%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			}
		},
		"desktopBrowser": {
			"design": "background",
			"backgroundColor": "#a8c9e8",
			"backgroundRadius": 0.45,
			"imageScale": 0.6
		},
		"windows": {
			"pictureAspect": "whiteSilhouette",
			"backgroundColor": "#5f99d2",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			}
		},
		"androidChrome": {
			"pictureAspect": "backgroundAndMargin",
			"margin": "29%",
			"backgroundColor": "#a8c9e8",
			"themeColor": "#a8c9e8",
			"manifest": {
				"name": "aserto",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "silhouette",
			"themeColor": "#5f99d2"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": false,
		"usePathAsIs": false
	}
}

