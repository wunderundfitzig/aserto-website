{
  "name": "aserto-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-export-optimize-images",
    "start": "next start",
    "lint": "next lint",
    "images:resize": "mkdir -p public/resized-images && node scripts/images-resize.js",
    "generate:favicons": "real-favicon generate favicon-description.json public/favicons/favicon-data.json public/favicons"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "fefe": "^3.2.0",
    "lodash": "^4.17.21",
    "next": "^14.0.4",
    "poisson-disk-sampling": "^2.2.2",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^6.0.2",
    "seedrandom": "^3.0.5",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/d3-shape": "^3.0.2",
    "@types/lodash": "^4.14.168",
    "@types/poisson-disk-sampling": "^2.2.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/seedrandom": "^3.0.1",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "cli-real-favicon": "^0.0.8",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "next-export-optimize-images": "^3.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
