{
  "name": "aserto-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:code": "next build",
    "build:export": "next export",
    "build": "npm run images:resize && npm run build:code && npm run build:export",
    "start": "next start",
    "lint": "next lint",
    "images:resize": "mkdir -p public/resized-images && node scripts/images-resize.js",
    "generate:favicons": "real-favicon generate favicon-description.json public/favicons/favicon-data.json public/favicons"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "fefe": "^3.2.0",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "poisson-disk-sampling": "^2.2.2",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.2",
    "seedrandom": "^3.0.5",
    "sharp": "^0.29.1",
    "uninstall": "^0.0.0"
  },
  "devDependencies": {
    "@types/d3-shape": "^3.0.2",
    "@types/lodash": "^4.14.168",
    "@types/poisson-disk-sampling": "^2.2.0",
    "@types/react": "^17.0.3",
    "@types/seedrandom": "^3.0.1",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "cli-real-favicon": "^0.0.8",
    "eslint": "^7.23.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "typescript": "^4.3.5"
  }
}
